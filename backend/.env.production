# Production Environment Variables
NODE_ENV=production
PORT=5000

# Database
MONGODB_URI=your_production_mongodb_connection_string_here

# JWT Settings - IMPORTANT: Change this to a secure random string in production
JWT_SECRET=change_this_to_a_secure_32_character_random_string_for_production
